{
  "name": "vwo-fme-node-sdk",
  "version": "1.2.3",
  "description": "VWO Node SDK for Feature Management and Experimentation",
  "main": "dist/server/index",
  "browser": "dist/client/vwo-fme-node-sdk",
  "exports": {
    ".": {
      "node": {
        "types": "./dist/server/index.d.ts",
        "import": "./dist/server/index",
        "require": "./dist/server/index",
        "default": "./dist/server/index"
      },
      "default": {
        "types": "./dist/server/index.d.ts",
        "import": "./dist/client/vwo-fme-node-sdk.js",
        "require": "./dist/client/vwo-fme-node-sdk.min.js",
        "default": "./dist/client/vwo-fme-node-sdk.min.js"
      }
    },
    "./node": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index",
      "require": "./dist/server/index",
      "default": "./dist/server/index"
    },
    "./browser": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/vwo-fme-node-sdk.js",
      "require": "./dist/vwo-fme-node-sdk.min.js",
      "default": "./dist/vwo-fme-node-sdk.min.js"
    }
  },
  "types": "dist/server/index.d.ts",
  "scripts": {
    "build": "yarn build:dev && yarn build:prod",
    "build:dev": "webpack --mode=development",
    "build:dev-watch": "webpack --mode=development --watch",
    "build:dev-analyze": "webpack --mode=development --env analyze=1",
    "build:prod-analyze": "webpack --mode=production --env analyze=1",
    "build:prod": "webpack --mode=production",
    "check:license": "yarn check:versions && node -e 'require(\"./scripts/check-license\")'",
    "check:versions": "node -e 'require(\"./scripts/check-versions\")'",
    "demo": "nodemon --inspect=0.0.0.0:9229 --legacy-watch --ignore node_modules demo/index.js",
    "demo:server": "nodemon --inspect=0.0.0.0:9229 --legacy-watch --ignore node_modules demo/server.js",
    "lint": "node -e 'require(\"./scripts/check-versions\")([\"node_lint\"])' && eslint lib/ --fix",
    "lint:errors-only": "node -e 'require(\"./scripts/check-versions\")([\"node_lint\"])' && eslint **/*.ts' --fix --quiet",
    "prepare": "husky",
    "prettier": "prettier -w lib/**/*.ts *.md",
    "test:dev": "node --inspect-brk node_modules/jest/bin/jest.js --watch --runInBand --debug --colors --errorOnDeprecated",
    "test:prod": "jest --runInBand --colors --errorOnDeprecated",
    "test:coverage": "jest --coverage --coverageDirectory=coverage && cat ./coverage/lcov.info",
    "tsc": "yarn check:versions && tsc -w",
    "typedoc": "typedoc lib/*.ts lib/**/*.ts lib/**/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wingify/vwo-fme-node-sdk"
  },
  "author": "VWO developers",
  "license": "Apache-2.0",
  "files": [
    "dist/",
    "package.json",
    "yarn.lock",
    "lib/**/*",
    "LICENSE",
    "README.md",
    "CONTRIBUTING.md",
    "CHANGELOG.md",
    "NOTICE"
  ],
  "dependencies": {
    "murmurhash": "^2.0.1",
    "superstruct": "^0.14.x",
    "uuid": "^9.0.1",
    "vwo-fme-sdk-log-messages": "^0.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.24.1",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.2.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "eslint": "^9.2.0",
    "express": "^4.19.2",
    "globals": "^15.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^2.0.6",
    "prettier": "^3.2.5",
    "semver": "^7.6.0",
    "shelljs": "^0.8.5",
    "ts-loader": "^9.5.1",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0",
    "vwo-fme-sdk-e2e-test-settings-n-cases": "^1.1.1",
    "webpack": "^5.91.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4"
  },
  "lint-staged": {
    "**/*.{ts,json,md}": [
      "prettier --write"
    ]
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 8.9.0",
    "yarn": ">= 1.22.17",
    "node_lint": ">= 18.18.0"
  }
}
